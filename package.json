		"url": "https://github.com/fwilkerson/clean-set/issues"
	},
	"homepage": "https://github.com/fwilkerson/clean-set#readme",
	"devDependencies": {
		"codecov": "^3.0.2",
		"esm": "^3.0.46",
		"microbundle": "^0.4.4",
		"nyc": "^12.0.2",
		"prettier": "^1.13.4",
		"tap-difflet": "^0.7.1",
		"tape": "^4.9.0"
	}
